<?xml version="1.0" ?>
<robot name = "pr_robot" xmlns:xacro="http://www.ros.org/wiki/xacro">

    <link name = "base_link">
    <visual>
        <origin rpy = "0 0 0" xyz = "0 0 2.5" />
        <geometry>
            <cylinder radius = "1.5" length  = "5" />
        </geometry>
    </visual>
    </link>

    <joint name = "joint1" type = "prismatic">
        <parent link = "base_link"/>
        <child link = "link1"/>
            <axis xyz = "0 0 1"/>
            <origin rpy = "0 0 0" xyz = "0 0 1" />
        <limit effort = "1000.0" lower = "0" upper = "4" velocity="0.5"/>
    </joint>

    <link name = "link1">
    <visual>
        <origin rpy = "0 0 0" xyz = "0 0 2.5" />
        <geometry>
            <cylinder radius = "1" length  = "5" />
        </geometry>
    </visual>
    </link>

    <joint name = "joint2" type = "revolute">
        <parent link = "link1"/>
        <child link = "link2"/>
            <axis xyz = "0 0 1"/>
            <origin rpy = "0 0 0" xyz = "0 0 0" />
        <limit effort = "1000.0" lower = "-3.14" upper = "3.14" velocity="1"/>
    </joint>

    <link name = "link2">
    <visual>
        <origin rpy = "0 0 0" xyz = "2.5 0 5.5" />
        <geometry>
            <box size = "5 1 1"/>
        </geometry>
    </visual>
    </link>
  </robot>